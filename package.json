  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^18.11.10",
    "prettier": "^2.8.0",
    "rm-modules": "^1.0.2",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  }
}
